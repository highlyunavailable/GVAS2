    break;
    default:
      fillLevelLoopcount = 0;
    break;
  }
  if (pattern != patternNone) {drawpercent = String.Format("{0:N0}%",percent);}
  if (!paints[bar]) {
    if (invertColor) {
      Colors[bar] = cvar(percent/100);
      backg = 0.7f - (Math.Abs(40-percent)*0.03f);
    } else {
      Colors[bar] = cvar(1-(percent/100));
