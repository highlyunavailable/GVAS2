    if (reference &gt; 0) {
      percent = 100 * cur_cap / reference;
    } else {
      percent = 100 * (float)tank.FilledRatio;
    }
    arr[1] += percent;
    sumAll[1] += percent;
    arr[2] += cur_cap;
  }
}

